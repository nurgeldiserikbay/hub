{
	"version": 2,
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/static-build",
			"config": {
				"distDir": ".output/public"
			}
		}
	],
	"headers": [
		{
			"source": "/(.*\\.css)$",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=3600"
				},
				{
					"key": "Content-Type",
					"value": "text/css"
				}
			]
		},
		{
			"source": "/(.*\\.js)$",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=3600"
				},
				{
					"key": "Content-Type",
					"value": "application/javascript"
				}
			]
		},
		{
			"source": "/(.*\\.json)$",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=3600"
				},
				{
					"key": "Content-Type",
					"value": "application/json"
				}
			]
		},
		{
			"source": "/(.*\\.(jpg|jpeg|png|gif|ico|svg))$",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=3600"
				}
			]
		},
		{
			"source": "/(.*\\.(woff|woff2|ttf|eot))$",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=3600"
				}
			]
		}
	]
}
